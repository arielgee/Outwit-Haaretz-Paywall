{
	"manifest_version": 2,
	"browser_specific_settings": {
		"gecko": {
			"id": "outwit-haaretz-paywall@webExtension.arielg.il",
			"strict_min_version": "68.0"
		}
	},
	"name": "Outwit Haaretz Paywall",
	"short_name": "OHP",
	"version": "1.3",
	"description": "Outwit the Haaretz paywall.",
	"author": "arielg",
	"permissions": [
		"*://www.haaretz.co.il/*",
		"*://www.themarker.com/*",
		"*://www-haaretz-co-il.cdn.ampproject.org/*",
		"*://www-themarker-com.cdn.ampproject.org/*",
		"tabs",
		"webRequest",
		"webRequestBlocking"
	],
	"icons": {
		"16": "/icons/outwit-16.png",
		"32": "/icons/outwit-32.png",
		"48": "/icons/outwit-48.png"
	},
	"browser_action": {
		"browser_style": false,
		"default_title": "OHP",
	},
	"commands": {
		"kb-ignore-next-request": {
			"suggested_key": {
				"default": "Alt+Shift+Z"
			},
			"description": "Ignore next Request"
		}
	},
	"background": {
		"scripts": [
			"/background.js"
		]
	}
}
