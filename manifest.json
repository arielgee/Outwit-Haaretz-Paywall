{
	"manifest_version": 2,
	"browser_specific_settings": {
		"gecko": {
			"id": "outwit-haaretz-paywall@webExtension.arielg.il",
			"strict_min_version": "68.0"
		}
	},
	"name": "Outwit Haaretz Paywall",
	"short_name": "OHP",
	"version": "1.4",
	"description": "Outwit the Haaretz paywall.",
	"author": "arielg",
	"permissions": [
		"*://www.haaretz.co.il/*",
		"*://www.themarker.com/*",
		"*://www-haaretz-co-il.cdn.ampproject.org/*",
		"*://www-themarker-com.cdn.ampproject.org/*",
		"tabs",
		"webRequest",
		"webRequestBlocking"
	],
	"icons": {
		"16": "/icons/outwit.svg",
		"32": "/icons/outwit.svg",
		"48": "/icons/outwit.svg",
		"64": "/icons/outwit.svg",
		"96": "/icons/outwit.svg"
	},
	"browser_action": {
		"browser_style": false,
		"default_title": "OHP",
		"default_icon": "/icons/outwit.svg"
	},
	"commands": {
		"state-enable": {
			"suggested_key": {
				"default": "Alt+Shift+A"
			},
			"description": "Enable"
		},
		"state-ignore-next-req": {
			"suggested_key": {
				"default": "Alt+Shift+Z"
			},
			"description": "Ignore next request"
		},
		"state-disable": {
			"suggested_key": {
				"default": "Alt+Shift+X"
			},
			"description": "Disable"
		}
	},
	"background": {
		"scripts": [
			"/background.js"
		]
	}
}
